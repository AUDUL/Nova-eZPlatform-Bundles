<table class="table table-striped table-hover table-sm">
    <tr>
        <th>Recipient counts</th>
        <th>Started</th>
        <th>Ended</th>
    </tr>
    {% for item in items %}
        <tr>
            <td>{{ item.emailSentCount }}</td>
            <td>{{ item.started|localizeddate(novaezmailing.dateformat.date, novaezmailing.dateformat.date) }}</td>
            <td>{{ item.ended|localizeddate(novaezmailing.dateformat.date, novaezmailing.dateformat.date) }}</td>
        </tr>
    {% endfor %}
</table>
